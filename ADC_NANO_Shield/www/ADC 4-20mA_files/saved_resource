    <script type="text/javascript">
        function convertToSlug(text) {
            return text.toLowerCase().replace(/[^\w ]+/g, '').replace(/ +/g, '-');
        }

        function popud3dModel(width, height) {
            var top = (screen.height - height) / 2 - 50;
            var left = (screen.width - width) / 2 - 10;
            var w = window.open(
                '/nanoshields/modulos/adc-4-20/3d?width=' + width + '&height=' + height,
                'model',
                'width=' + width + ',' +
                'height=' + height + ',' +
                'top=' + top + ',' +
                'left=' + left + ',' +
                'resizable=false'
            );
            if (window.focus) {
                w.focus();
            }
            return false;
        }

        $(function() {
            // Move img title to data-title
            $('img[title]').each(function() {
                $this = $(this);
                $.data(this, 'title', $this.attr('title'));
                $this.removeAttr('title');
            });

            // Move lightbox carousel to the clicked image
            $('.page-carousel-image').click(function() {
                $('#lightbox-carousel').carousel($(this).data('position'));
                $('#lightbox-carousel').carousel('pause');
            });

            // Open lightbox for image within the page contents
            $('.description p img').click(function() {
                $('#lightbox-content img').attr('src', $(this).data('title'));
                $('#lightbox-content').lightbox();
            });

            // Open popup for 3D model

                $('.model-btn .btn').click(function() {
                    popud3dModel(800, 600);
                });


            // Add bootstrap table classes to tables within the markdown content set correct text-align to cells
            $('.description table').addClass('table table-bordered');
            $('.description table [align="center"]').css('text-align', 'center');

            // Process section headings
            count = 0;
            $('.description h2, .description h3').each(function() {
                // Create collapsible <div> after heading
                $collapse = $('<div/>').addClass('collapse').attr('id', 'collapse' + count);
                $(this).after($collapse).attr('data-toggle', 'collapse').attr('data-target', '#collapse' + count);

                // Move all elements after each heading into the collapsible <div>
                if ($(this).prop("tagName") === 'H2') {
                    $collapse.nextUntil('h1, h2').appendTo($collapse);
                } else {
                    $collapse.nextUntil('h1, h2, h3').appendTo($collapse);
                }

                // Make each <h2> in the description collapsible
                $(this).addClass('section-heading');

                // Expand flagged sections
                var title = $(this).text();
                if ($(this).text()[0] === '!') {
                    title = title.substring(1);
                    $(this).text(title);
                    $collapse.addClass('in');
                } else {
                    $(this).addClass('collapsed');
                }

                // Add anchor to each header
                $(this).addClass('anchor').prepend('<span id="' + convertToSlug(title) + '"></span>');

                count++;
            });
        });
    </script>


<script type="text/javascript">
    function submitNewsletterForm() {
        ga('send', 'event', 'Click', 'Subscribe', {
            'hitCallback': function() {
                $('#mc-embedded-subscribe-form').submit();
            }
        });

        return !window.ga;
    }

    $(function () {
        $('#mc-embedded-subscribe').on('click', submitNewsletterForm);

        $('#mce-EMAIL').keypress(function (e) {
            if (e.which == 13) {
                submitNewsletterForm();
                return false;
            }
        });
    });
</script>

